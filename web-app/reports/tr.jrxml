	<subDataset name="Movimientos" uuid="481ed7f6-caa6-4ba4-b893-77fbc12418c9">
		<parameter name="movement_id_m" class="java.lang.Long" isForPrompting="false"/>
		<queryString>
			<![CDATA[select p1.movement_id,a1.name as account_from, a2.name as account_to, p1.payment_date,p1.amount,p1.note
from payment p1
left join payment p2 on p1.payments_idx +1 = p2.payments_idx
left join account a1 on p1.account_id = a1.id
left join account a2 on p2.account_id = a2.id
where p1.movement_id = $P{movement_id_m} and p2.movement_id = $P{movement_id_m}
and p1.payments_idx % 2 = 0]]>
		</queryString>
